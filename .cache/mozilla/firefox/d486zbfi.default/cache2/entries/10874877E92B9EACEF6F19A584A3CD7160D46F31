        <div class="form" data-bind="visible: !loading()">
            <div data-bind="viewmodel: documentationTypeDropDown"></div>
            <div class="dev-id-textbox" data-bind="viewmodel: offerTypeDropDown"></div>

            <div class="dev-id-textbox" data-bind="viewmodel: documentationIdTextBox"></div>
