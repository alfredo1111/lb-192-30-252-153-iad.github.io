<script type="text/javascript">
    //FPTI tracking for terms button and cancel link
    $('.trackTerms').click(function() {
        if(typeof fpti != "undefined") {
            fpti.pgrp = "main:ppcredit:apply::termsandconditions";
            fpti.pglk = fpti.page + "|" + $('.trackTerms').attr('name');

            if (typeof PAYPAL.analytics != "undefined") {
                PAYPAL.pta = PAYPAL.analytics.instance.recordClick();
            }
        }
    });

    //FPTI tracking for prompt cancel link
    $('.trackPrompt').click(function() {
        if(typeof fpti != "undefined") {
            fpti.pgrp = "main:ppcredit:apply::termsandconditions::prompt";
            fpti.pglk = fpti.page + "|" + $('.trackPrompt').attr('name');

            if (typeof PAYPAL.analytics != "undefined") {
                PAYPAL.pta = PAYPAL.analytics.instance.recordClick();
            }
        }

    });
</script>
