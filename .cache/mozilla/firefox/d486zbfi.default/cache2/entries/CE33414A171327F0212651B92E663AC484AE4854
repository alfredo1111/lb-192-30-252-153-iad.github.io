<td class="crayon-code"><div class="crayon-pre" style="font-size: 14px !important; line-height: 20px !important; -moz-tab-size:4; -o-tab-size:4; -webkit-tab-size:4; tab-size:4;"><div class="crayon-line" id="crayon-5bd5fbc882392975761113-1"><span class="crayon-ta">&lt;?php</span></div><div class="crayon-line" id="crayon-5bd5fbc882392975761113-2"><span class="crayon-st">use</span><span class="crayon-h"> </span><span class="crayon-i">SparkPost</span><span class="crayon-sy">\</span><span class="crayon-i">SparkPost</span><span class="crayon-sy">;</span></div><div class="crayon-line" id="crayon-5bd5fbc882392975761113-3"><span class="crayon-st">use</span><span class="crayon-h"> </span><span class="crayon-i">GuzzleHttp</span><span class="crayon-sy">\</span><span class="crayon-i">Client</span><span class="crayon-sy">;</span></div><div class="crayon-line" id="crayon-5bd5fbc882392975761113-4"><span class="crayon-st">use</span><span class="crayon-h"> </span><span class="crayon-i">Http</span><span class="crayon-sy">\</span><span class="crayon-i">Adapter</span><span class="crayon-sy">\</span><span class="crayon-i">Guzzle6</span><span class="crayon-sy">\</span><span class="crayon-e">Client </span><span class="crayon-st">as</span><span class="crayon-h"> </span><span class="crayon-i">GuzzleAdapter</span><span class="crayon-sy">;</span></div><div class="crayon-line" id="crayon-5bd5fbc882392975761113-5">&nbsp;</div><div class="crayon-line" id="crayon-5bd5fbc882392975761113-6"><span class="crayon-v">$httpClient</span><span class="crayon-h"> </span><span class="crayon-o">=</span><span class="crayon-h"> </span><span class="crayon-r">new</span><span class="crayon-h"> </span><span class="crayon-e">GuzzleAdapter</span><span class="crayon-sy">(</span><span class="crayon-r">new</span><span class="crayon-h"> </span><span class="crayon-e">Client</span><span class="crayon-sy">(</span><span class="crayon-sy">)</span><span class="crayon-sy">)</span><span class="crayon-sy">;</span></div><div class="crayon-line" id="crayon-5bd5fbc882392975761113-7"><span class="crayon-v">$sparky</span><span class="crayon-h"> </span><span class="crayon-o">=</span><span class="crayon-h"> </span><span class="crayon-r">new</span><span class="crayon-h"> </span><span class="crayon-e">SparkPost</span><span class="crayon-sy">(</span><span class="crayon-v">$httpClient</span><span class="crayon-sy">,</span><span class="crayon-h"> </span><span class="crayon-sy">[</span><span class="crayon-s">'key'</span><span class="crayon-h"> </span><span class="crayon-o">=</span><span class="crayon-o">&gt;</span><span class="crayon-h"> </span><span class="crayon-s">'&lt;YOUR API KEY&gt;'</span><span class="crayon-sy">]</span><span class="crayon-sy">)</span><span class="crayon-sy">;</span></div><div class="crayon-line" id="crayon-5bd5fbc882392975761113-8">&nbsp;</div><div class="crayon-line" id="crayon-5bd5fbc882392975761113-9"><span class="crayon-v">$sparky</span><span class="crayon-o">-&gt;</span><span class="crayon-e">setOptions</span><span class="crayon-sy">(</span><span class="crayon-sy">[</span><span class="crayon-s">'async'</span><span class="crayon-h"> </span><span class="crayon-o">=</span><span class="crayon-o">&gt;</span><span class="crayon-h"> </span><span class="crayon-t">false</span><span class="crayon-sy">]</span><span class="crayon-sy">)</span><span class="crayon-sy">;</span></div><div class="crayon-line" id="crayon-5bd5fbc882392975761113-10"><span class="crayon-v">$results</span><span class="crayon-h"> </span><span class="crayon-o">=</span><span class="crayon-h"> </span><span class="crayon-v">$sparky</span><span class="crayon-o">-&gt;</span><span class="crayon-i">transmissions</span><span class="crayon-o">-&gt;</span><span class="crayon-e">post</span><span class="crayon-sy">(</span><span class="crayon-sy">[</span></div><div class="crayon-line" id="crayon-5bd5fbc882392975761113-11"><span class="crayon-h">&nbsp;&nbsp;</span><span class="crayon-s">'options'</span><span class="crayon-h"> </s